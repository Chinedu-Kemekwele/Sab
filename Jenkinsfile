pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building a code'
            }
        }
        stage('Pre-test') {
            steps {
                echo 'Pre-testing our code'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing code'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploy code'
            }
        }
    }
}
